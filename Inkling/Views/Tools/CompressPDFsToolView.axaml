<UserControl
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:toolsvm="clr-namespace:Inkling.ViewModels.Tools"
	xmlns:views="clr-namespace:Inkling.Views"
	xmlns:toolsviews="clr-namespace:Inkling.Views.Tools"
	x:Class="Inkling.Views.Tools.CompressPDFsToolView"
	x:DataType="toolsvm:CompressPDFsToolViewModel"
>
	<StackPanel Spacing="20">
		<views:FilePickerView DataContext="{Binding FilePicker}"/>
		<views:ChoiceBarView DataContext="{Binding CompressionQuality}"/>
	</StackPanel>
</UserControl>
